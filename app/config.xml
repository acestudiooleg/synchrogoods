<?xml version='1.0' encoding='utf-8'?>
<widget id="org.apache.cordova.synchrogoods" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Synchro Goods</name>
    <description>
      Live list of goods
    </description>
    <author email="acestudiooleg@gmail.com" href="http://cordova.io">
        Oleg Sudavnyi
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <icon density="mdpi" height="57" platform="android" src="res/android/red_truck.png" width="57" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
